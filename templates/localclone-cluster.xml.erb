<!DOCTYPE localclone SYSTEM "<%= dtd %>">
<localclone name="localclone2disk" source="disk" destination="disk">
  <cluster name="<%= name %>" sourcesuffix="" destsuffix="C"/>
  <sourcedisks>
     <bootdisk name="<%= sourcebootdisk %>"/>
     <rootdisk name="<%= sourcerootdisk %>"/>
  </sourcedisks>
  <destdisks>
    <bootdisk name="<%= destbootdisk %>" samesize="1"/>
    <rootdisk name="<%= destrootdisk %>" samesize="1"/>
  </destdisks>
  <kernel version="<%= kernelrelease %>"/>
</localclone>
